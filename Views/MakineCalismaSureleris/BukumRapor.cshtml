@model IEnumerable<PolyteksEnerjiYonetimSistemi.Models.Makine>

@{
    ViewBag.Title = "BukumRapor";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="card">
    <div class="card-body" style="background-color:cadetblue">
        <center><h2 style="color:black">BÜKÜM @DateTime.Now.AddYears(-1).Year YILI RAPORU</h2></center>

        <div class="container">
            <div id="accordion" class="accordion">
                <div class="card mb-0">
                    <div class="card-header collapsed" data-toggle="collapse" href="#collapseOne1">
                        <a class="card-title">
                            <b>OCAK</b>
                        </a>
                    </div>
                    <div id="collapseOne1" class="card-body collapse" data-parent="#accordion">
                        <div class="card-body">
                            <table class="table table-bordered">
                                <tr style="background-color: darkseagreen">

                                    <th>
                                        @Html.DisplayNameFor(model => model.MakineAdi)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.KWH)
                                    </th>

                                    <th></th>

                                </tr>

                                @foreach (var item in Model)
                                {
                                    <tr style="background-color: lightskyblue" class="Search">

                                        <td>
                                            @Html.DisplayFor(modelItem => item.MakineAdi)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.KWH)
                                        </td>

                                        <td>
                                            <table class="table table-bordered table-info">
                                                <thead>
                                                    <tr style="background-color: darkseagreen">
                                                        <th>
                                                            YIL
                                                        </th>


                                                        <th>
                                                            ÇALIŞMA SÜRESİ
                                                        </th>
                                                        <th>
                                                            KAYIT EDEN
                                                        </th>


                                                        <th>
                                                            KAYIT TARİHİ
                                                        </th>
                                                        <th>
                                                            TOPLAM TÜKETİM
                                                        </th>

                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    @foreach (var detay in item.MakineCalismaSureleri)
                                                    {
                                                        if (detay.Ay == 1 && detay.Yil - 1 == DateTime.Now.AddYears(-1).Year)
                                                        {
                                                            var a = item.KWH * detay.CalismaSuresi;
                                                            <tr class="Search">
                                                                <td>@(detay.Yil)</td>


                                                                <td>@(detay.CalismaSuresi)</td>

                                                                <td> @(detay.KayitEden)</td>

                                                                <td>        @Convert.ToDateTime(detay.KayitTarihi).ToString("dd.MM.yyyy")</td>

                                                                <td> @(a.Value.ToString("N2"))</td>


                                                            </tr>
                                                        }
                                                    }

                                                </tbody>
                                            </table>
                                        </td>


                                    </tr>
                                }


